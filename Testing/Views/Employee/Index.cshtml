@model IEnumerable<Employee>

<h2>Employees</h2>
<div style="display: block">
    <a href="/Employee/InsertEmployee/">Create a New Employee</a>
</div>

<table class="table">
    <tr>
        <th>ID</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th>PhoneNumber</th>
        <th>Title</th>
        <th>DOB</th>
    </tr>

    @foreach (var employee in @Model)
    {
        <tr>
            <td><a href=/Employee/ViewEmployee/@employee.EmployeeID>@employee.EmployeeID</a></td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.PhoneNumber</td>
            <td>@employee.Title</td>
            
        </tr>
    }

</table>